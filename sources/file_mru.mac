/*ファイル履歴
*/

$label=getarg(0);
call $label;
endmacro $$return;

get_property:
	//ソースのプロパティをini形式で返す
	return R"([property]
name=file_mru
description=candidates from file MRU list
default_action=common
candidate_type=list
)";

gather_candidates:
	/*候補を返す
	*/
	$$candidate_list="";
	##i=0;
	while(##i<filehistcount){
		$$candidate_list = $$candidate_list + getfilehist(##i) + "\n";
		##i = ##i + 1;
	}
	return $$candidate_list;
